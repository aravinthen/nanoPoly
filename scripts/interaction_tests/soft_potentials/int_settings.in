variable structure index int_structure.in                                             
variable simulation index int_settings.in                                            
                                                                                      
# Initialization settings                                                             
units              lj                                                                 
boundary           p p p                                                              
atom_style         bond                                                       
log                log.${simulation}.txt                                            
                                                                                      
# read data from object into file                                                     
read_data          ${structure}                                                     

# define interactions                                                                 
neighbor      4.0 multi                                                           
neigh_modify  every 10 one 10000                                         
pair_style    lj/cut 1.5                                                         
pair_coeff    1 1 0.2 0.1                                  
pair_style    lj/cut 1.5                                                         
pair_coeff    1 2 0.2 0.1                                  
pair_style    lj/cut 1.5                                                         
pair_coeff    2 2 0.5 0.1                                  

bond_style    fene  
special_bonds fene  
bond_coeff    1 30 1.5 1.0 1.0 
compute         1 all stress/atom NULL 

